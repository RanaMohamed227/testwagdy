pipeline {
    agent any


    environment {
        PYTHON_PATH = 'C:\\Program Files\\Python38\\python'

        }

    stages {

        stage('Run Python Script') {
            steps {
                script {
                    def result = bat(script: "${env.PYTHON_PATH} select.py", returnStdout: true).trim()
                    echo result
                }
            }
        }
    }
}
